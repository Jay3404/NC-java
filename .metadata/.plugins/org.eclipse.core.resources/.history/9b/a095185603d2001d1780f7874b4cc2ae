	static void Prob01() {
		int a = 10;
		int b = 3;
		
		int add = a + b;
		int substract = a - b;
		int multiply = a * b;
		double divide = a / b;
		
		System.out.println("add : " + add + ", substract : " + substract + ", multiply : " + multiply + ", divide : " + divide );
	}
	
