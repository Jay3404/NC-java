package chap98_homework.nc230404.clazz;

public class NumberPrinter {
	private int num;
	
	public void numPrint1(int a) {
		this.num = a;
		
		for(int i = this.num; i <= 50; i++) {
			if(i % 3 == 0) {
				System.out.println(i);
			}
		}
	}
	
	public void numPrint2(int a) {
		this.num = a;
		
		for(int i = this.num; i <= 100; i++) {
			if(i % 5 == 0 && i % 7 == 0) {
				System.out.println(i);
			}
		}
	}
}
